{
  "name": "Always on Image and Video Remover",
  "version": "1.0",
  "description": "Turn on the extension and find no images or videos on any of the pages you open.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "css": ["pageConcealer.css"],
      "js": ["pageConcealer.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["removeMedia.js"]
    }
  ],
  "manifest_version": 2
}
